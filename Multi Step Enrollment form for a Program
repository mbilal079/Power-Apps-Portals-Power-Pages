    function submitApplication() {
        try {
            console.log("Submit Called");

            var formFieldsData =  getFormData();  
            formFieldsData.cfa_enrollmentstatus = "100000004"; //pending
            formFieldsData.cfa_importid = "DSYA Enrollment Form";
 
            console.log("Got Form data");

            $(".preloader").removeClass("hideit");
            debugger;
            webapi.safeAjax({
                type: "POST",
                url: "/_api/contacts",
                contentType: "application/json",
                data: JSON.stringify(formFieldsData),
                success: function (res, status, xhr) {
                    console.log("entityID Of Applicant: " + xhr.getResponseHeader("entityid"));
                    var entityid = xhr.getResponseHeader("entityid");
                    attachSign(entityid);
                },
                error: function (res, status, xhr) {
                    console.log("Error in creating");
                    console.log(res.responseText);
                    createErrorFormLog(formFieldsData, res.responseText);
                }
            });
            setTimeout(function () {
                $(".preloader").addClass("hideit");
            }, 120000);
        }
        catch (ex) {
            alert("Error => " + JSON.stringify(ex));
        }
    }
    function updateContact(contactid) {
        try {
            $(".preloader").removeClass("hideit");

            var formFieldsData =  getFormData();

            webapi.safeAjax({
                type: "PATCH",
                url: "/_api/contacts(" + contactid + ")",
                contentType: "application/json",
                data: JSON.stringify(formFieldsData),
                success: function (res, status, xhr) {
                    console.log("entityID Of Applicant: " + xhr.getResponseHeader("entityid"));
                    var entityid = xhr.getResponseHeader("entityid");
                    attachSign(entityid);
                },
                error: function (res, status, xhr) {
                    console.log('Error in udpating');
                    console.log(res.responseText); 
                    createErrorFormLog(formFieldsData, res.responseText);
                }
            });
            setTimeout(function () {
                $(".preloader").addClass("hideit");
            }, 120000);
        }
        catch (ex) {
            alert("Error => " + JSON.stringify(ex));
        }
    }

    function createErrorFormLog(formBody, error) {
        console.log("************************");  
        let tableProperties = {
            cfa_name : ((new Date()).toISOString()),
            cfa_formbody: ((formBody) ? JSON.stringify(formBody) : null),
            cfa_error: ((error) ? JSON.stringify(error) : null)
        }
        webapi.safeAjax({
            type: "POST",
            url: "/_api/cfa_formissueses", //
            contentType: "application/json",
            data: JSON.stringify(tableProperties),
            success: function (res, status, xhr) {
                window.open('/thank-you', "_self");
            },
            error: function (res, status, xhr) {
                console.log("Form issues error");
                console.log(res);
                alert("Error: Please copy the below error and send to sfaizullah@computingforall.org, mbilal@computingforall.org \n =============\n  " + JSON.stringify(res.responseText));
            }
        });
    }

    function attachSign(id) {
        var $sigdiv = $("#signature");
        var datapair = $sigdiv.jSignature("getData", "svgbase64");
        console.log("datapair =" + datapair[1]);
        console.log("contact id =" + id);
        webapi.safeAjax({
            type: "POST",
            url: "/_api/annotations", //TODO: check the URL is right? 
            contentType: "application/json",
            data: JSON.stringify({
                "objectid_contact@odata.bind": "/contacts(" + id + ")",
                "objecttypecode": 'contact',
                "documentbody": datapair[1],
                "isdocument": true,
                "filename": $("#fname").val() + " " + $("#fname").val() + " - Signature.svg",
                "mimetype": "image/svg+xml",
                "subject": $("#fname").val() + " " + $("#fname").val() + " - Signature"
            }),
            success: function (res, status, xhr) {
                console.log("entityID of Annotation: " + xhr.getResponseHeader("entityid"));
                console.log("Signature Uploaded successfully as Notes in Contact");
                $(".preloader").addClass("hideit");
                window.open('/thank-you', "_self");
            }
        });
    }
</script>
