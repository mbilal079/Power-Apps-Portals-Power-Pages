{% include 'jSignature.min.js' %}
<script>
    console.log("Version: 1.0.1");
    $(document).ready(function () {
        $("#signature").jSignature();

        setTimeout(function () {
            $(".signaturebox").addClass("hideit");
        }, 1400);

        $(".virtualsign90").click(function () {
            $(".signaturebox").removeClass("hideit");
            $(".signaturebox").css("visibility", "visible");
            $(".signaturebox").css("height", "auto");
            $(".cfaenrollmentform").addClass("hideit");
            $(".signaturebox").removeClass("hideit");
        });
    });

    function signinit() { };

    function loadcanvas() {
        $("#signature").html("");
        $("#signature").jSignature();
    };

    function reset() {
        var $sigdiv = $("#signature");
        $sigdiv.jSignature("reset");
    };

    function selectit() {
        $(".signaturebox").css("visibility", "hidden");
        $(".signaturebox").css("height", "20px");
        $(".cfaenrollmentform").removeClass("hideit");
        $("#selectedimagebox").html("");
        /////////////
        var $sigdiv = $("#signature");
        var datapair = $sigdiv.jSignature("getData", "svgbase64");
        var i = new Image()
        i.src = "data:" + datapair[0] + "," + datapair[1]
        $(i).appendTo($("#selectedimagebox"))
    };
</script>

<script>
    //let formFieldsData;
    try {
        var formheight = 200;
        $(document).ready(function () {

            setTimeout(function () {
                $(".preloader").addClass("hideit");
            }, 1500);

            console.log("document is ready");
            $(".qfields").css("height", window.innerHeight - 320);

            //getContact();

            setInterval(function () {
                $(".qfields").css("height", window.innerHeight - 320);
            }, 2000);

            $(".continuebtn").click(function () {
                //$("#signature").jSignature();
                $(".qform").removeClass("hideit");
                $("#intcont").addClass("hideit");
                $(".preloader").removeClass("hideit");
                setTimeout(function () {
                    $(".preloader").addClass("hideit");
                }, 1800);
            });

            $(".closebtn").click(function () {
                if (confirm("Are you sure you want to Close the From?")) {
                    $(".qform").addClass("hideit");
                    $("#intcont").removeClass("hideit");
                    $(".preloader").removeClass("hideit");
                    setTimeout(function () {
                        $(".preloader").addClass("hideit");
                    }, 1800);
                } else {

                }
            });

            if ($(".q-1").hasClass("active")) {
                $(".backbtn").addClass("hideit");
            } else {
                $(".backbtn").removeClass("hideit");
            }

            $(".nextbtn").click(function () {
                //console.log("next clicked");
                $(".qcont").removeClass("animationnext");
                $(".qcont").removeClass("animationback");
                checkRequired();
            });

            $(".backbtn").click(function () {
                $(".qcont").removeClass("animationnext");
                $(".qcont").removeClass("animationback");
                console.log("back clicked");
                var nextq = $(".qdiv.active").prev();
                $(".qdiv.active").removeClass("active");
                //$(".qcont").addClass("animationback");
                $(nextq).addClass("active");

                manageNavBtns("back");
            });

            $("#wherehear").change(function () {
                if ($(this).val() == "6") {
                    $(".otherwheredidyouhear").removeClass("hideit");
                    $(".otherwheredidyouhearlable").html("Friend*");
                    $("#otherwherehear").addClass("required");
                }
                else if ($(this).val() == "7") {
                    $(".otherwheredidyouhear").removeClass("hideit");
                    $(".otherwheredidyouhearlable").html("Teacher*");
                    $("#otherwherehear").addClass("required");
                }
                else if ($(this).val() == "8") {
                    $(".otherwheredidyouhear").removeClass("hideit");
                    $(".otherwheredidyouhearlable").html("Other*");
                    $("#otherwherehear").addClass("required");
                }
                else {
                    $(".otherwheredidyouhear").addClass("hideit");
                    $("#otherwherehear").removeClass("required");
                    $("#otherwherehear").val("");
                }
            });

            $("#stydenttype").change(function () {
                if ($(this).val() == "100000000") {
                    $(".schoolinfo").removeClass("hideit");
                    $(".collegeinfo").addClass("hideit");
                    addRemoveRequired(".stydenttype_100000000", "add");
                    addRemoveRequired(".stydenttype_100000001", "remove");
                }
                else if ($(this).val() == "100000001") {
                    $(".schoolinfo").addClass("hideit");
                    $(".collegeinfo").removeClass("hideit");
                    addRemoveRequired(".stydenttype_100000000", "remove");
                    addRemoveRequired(".stydenttype_100000001", "add");
                }
                else {
                    $(".schoolinfo").addClass("hideit");
                    $(".collegeinfo").addClass("hideit");
                    addRemoveRequired(".stydenttype_100000000", "remove");
                    addRemoveRequired(".stydenttype_100000001", "remove");
                }
            });

            $("#haveajob").change(function () {
                if ($(this).val() == 'true') {
                    $(".ihavejob").removeClass("hideit");
                    addRemoveRequired(".haveajob_true", "add");
                    $(".qdiv.active .qfields").animate({
                        scrollTop: $(
                            '.qdiv.active .qfields').get(0).scrollHeight
                    }, 3000);
                }
                else {
                    $(".ihavejob").addClass("hideit");
                    addRemoveRequired(".haveajob_true", "remove");
                }
            });

            $("#consent_under18").change(function () {
                if ($(this).val() == 'true') {
                    $(".under18_consentSigndiv").removeClass("hideit");
                    addRemoveRequired(".consent_under18_true", "add");
                }
                else {
                    $(".under18_consentSigndiv").addClass("hideit");
                    addRemoveRequired(".consent_under18_true", "remove");
                }
            });

            $("#filling").change(function () {
                if ($(this).val() == '100000000') {
                    $(".under18").addClass("hideit");
                    $(".over18").removeClass("hideit");
                    addRemoveRequired(".filling_100000000", "add");
                    addRemoveRequired(".filling_100000001", "remove");
                }
                else if ($(this).val() == '100000001') {
                    $(".under18").removeClass("hideit");
                    $(".over18").addClass("hideit");
                    addRemoveRequired(".filling_100000001", "add");
                    addRemoveRequired(".filling_100000000", "remove");
                }
                else {
                    $(".under18").removeClass("hideit");
                    $(".over18").removeClass("hideit");
                    addRemoveRequired(".filling_100000000", "remove");
                    addRemoveRequired(".filling_100000001", "remove");
                }
            });

            $("#under18").change(function () {
                if ($(this).val() == 'true') {
                    $(".under18_section").removeClass("hideit");
                    addRemoveRequired(".under18_section", "add");

                }
                else {
                    $(".under18_section").addClass("hideit");
                    addRemoveRequired(".under18_section", "remove");
                }
            });

            $("input.required, select.required, textarea.required").change(function () {
                if ($(this).val() == "") {
                    $(this).addClass("error");
                }
                else {
                    $(this).removeClass("error");
                }
            });

            $("input.depend, select.depend").change(function () {
                //var dependent = $(this).attr("id")+"_"+$(this).val();
                //if(("."+dependent).hasClass("hide"))
                // {
                //$("."+dependent+" input, ."+dependent+" select").removeClass("required");
                // }
                // else
                // {
                //$("."+dependent+" input, ."+dependent+" select").addClass("required");
                //  }

            });
        });
    }
    catch (ex) {
        alert(ex);
    }

    function addRemoveRequired(identifier, actiontype) {
        if (actiontype == "add") {
            $(identifier + " input, " + identifier + " select").each(function () {
                $(this).addClass("required");
            })
        }
        else if (actiontype == "remove") {
            $(identifier + " input, " + identifier + " select").each(function () {
                $(this).removeClass("required");
            })
        }
    }

    function scrollUpQFields() {
        $(".qdiv.active .qfields").animate({
            scrollTop: 0
        }, 2200);
    }

    function updatePageNo() {
        var count = 0;
        $(".qform .qdiv").each(function () {
            count = count + 1;
            if ($(this).hasClass("active")) {
                console.log("page no " + count);
                $(".activepageno").text(count);
            }
        })
    }

    function manageNavBtns(key) {
        try {
            updatePageNo();
            scrollUpQFields();
            submitPageBackBtn();
            applyAnimation(key);
            if ($(".q-1").hasClass("active")) {
                $(".backbtn").addClass("hideit");
            }
            else {
                $(".backbtn").removeClass("hideit");
            }
            if ($(".submitpage").hasClass("active")) {
                $(".nextbtn").addClass("hideit");
                $(".submitbtn").removeClass("hideit");
            }
            else {
                $(".nextbtn").removeClass("hideit");
                $(".submitbtn").addClass("hideit");
            }
        }
        catch (ex) {
            alert(ex);
        }
    }

    function applyAnimation(key) {
        if (key == "next") {
            $(".qcont").addClass("animationnext");
        }
        else if (key == "back") {
            $(".qcont").addClass("animationback");
        }
    }

    function submitPageBackBtn() {
        if ($(".qdiv.active").hasClass("submitpage")) {
            $(".backbtn").addClass("submitpagebackbtn");
        }
        else {
            $(".backbtn").removeClass("submitpagebackbtn");
        }
    }
    
    function checkRequired() {
        var validation = true;
        $(".qdiv.active input.required, .qdiv.active select.required, .qdiv.active textarea.required").each(function () {
            //console.log("Value = "+$(this).val());
            if ($(this).val() == "" || $(this).val() == undefined || $(this).val() == null) {
                validation = false;
                //console.log("Value = "+$(this).val());
                $(this).addClass("error");
            }
        })


        if (validation) {
            var nextq = $(".qdiv.active").next(".qdiv");
            //var nextval = 
            //console.log("next length => "+nextq.length);
            //console.log("Validation true - Next Q => "+JSON.stringify(nextq));
            if (nextq.length > 0 && nextq != null && nextq != "" && nextq != undefined) {
                $(".qdiv.active").removeClass("active");
                $(nextq).addClass("active");
                manageNavBtns("next");
            }

        }
        else {
            alert("Please fill all required fields");
        }
    }
    //p22
    function makeRandString(length) {
        var result = '';
        var characters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';
        var charactersLength = characters.length;
        for (var i = 0; i < length; i++) {
            result += characters.charAt(Math.floor(Math.random() * charactersLength));
        }
        return result;
    }
    function confirmSubmit() {
        try {
            $(".preloader").removeClass("hideit");

            var validation = true;
            if (document.getElementById('selectedimagebox').children.length < 1) {
                validation = false;
                throw "Signature Required";
            }
            $(".qform input.required, .qform select.required, .qform textarea.required").each(function () {
                if ($(this).val() == "") {
                    $(this).addClass("error");
                    validation = false;
                }
            });
            if (validation == false) {
                throw "Sorry! some required fields are empty, please fill all required fields";
            }
            if (confirm("Are you sure you want to Submit the Application?")) {
                checkExistedContact();
            }
            else {

            }
        }
        catch (ex) {
            alert(ex);
            $(".preloader").addClass("hideit");
        }
    }

    function checkExistedContact() {
        console.log("Checking existing contact..");
        var url = "/_api/contacts?$select=firstname,contactid&$filter=emailaddress1 eq '" + $("#email").val() + "'";
        webapi.safeAjax({
            type: "GET",
            url: url,
            contentType: "application/json",
            success: function (res) {
                console.log("Get Contact in Success");
                if (res.value.length > 0) {
                    console.log("Email is " + $("#email").val());          
                    updateContact(res.value[0]["contactid"]);
                }
                else {
                    submitApplication();
                }
                //$(".loader-container").addClass("hideit");
            },
            error: function (res, status, xhr) {
                console.log("Error in finding contact");
                console.log(res.responseText); 
                createErrorFormLog(getFormData(), res.responseText);
            }
        })
    }

